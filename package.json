{
  "name": "clean-ts-api",
  "version": "1.1.0",
  "description": "API do Curso do Rodrigo Manguinho",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --coverage=false --silent --noStackTrace --runInBand",
    "test:verbose": "yarn jest --silent=false",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:integration": "yarn test --watch -c jest-integration-config.js",
    "test:staged": "yarn test --passWithNoTests --findRelatedTests",
    "test:ci": "yarn test --coverage=true",
    "prepare": "husky install",
    "build": "rimraf dist && tsc",
    "start:prd": "node dist/main/server.js",
    "start:dev": "yarn db:start && sucrase-node src/main/server.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down"
  },
  "keywords": [],
  "author": "joao.pedroasz00@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@swc/core": "^1.3.59",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "rimraf": "^5.0.1",
    "sucrase": "^3.32.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.5.0",
    "validator": "^13.9.0"
  }
}
